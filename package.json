{
  "name": "ericdudley",
  "version": "1.0.0",
  "description": "ericdudley.com, my personal website",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/*.html",
    "lint": "eslint src/**/*.{js,ts}",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "tsc": "tsc --watch"
  },
  "lint-staged": {
    "src/**/*": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericdudley/ericdudley.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ericdudley/ericdudley/issues"
  },
  "homepage": "https://github.com/ericdudley/ericdudley#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.4",
    "parcel-bundler": "^1.12.5",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "audio": "^1.2.0",
    "howler": "^2.2.3",
    "sass": "^1.37.5"
  }
}
